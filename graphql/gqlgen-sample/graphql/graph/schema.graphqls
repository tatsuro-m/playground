# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Post {
    id: ID!
    title: String!
    createdAt: Time!
    updatedAt: Time!
}

input NewPost {
    title:String!
}

input DeletePost {
    id: ID!
}

type Query {
    posts: [Post!]!
}

type Mutation {
    createPost(input: NewPost): Post!
    deletePost(input: DeletePost): Boolean!
}

scalar Time
